{
  "name": "rbh-vpnapp",
  "version": "1.0.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dist:win": "electron-builder --win",
    "dist:linux": "electron-builder --linux AppImage"
  },
  "dependencies": {
    "electron": "29.4.0",
    "axios": "1.7.2",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "electron-builder": "24.13.3"
  },
  "build": {
    "appId": "com.rbh.vpnapp",
    "productName": "RBH VpnApp",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "artifactName": "RBH-VpnApp-Setup-${version}.exe"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "artifactName": "RBH-VpnApp-${version}.AppImage"
    }
  }
}